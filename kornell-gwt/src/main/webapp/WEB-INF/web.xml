<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<welcome-file-list>
		<welcome-file>Kornell.html</welcome-file>
	</welcome-file-list>

	<!-- TODO: Consider removing in production -->
	<servlet>
		<servlet-name>jUnitHostImpl</servlet-name>
		<servlet-class>com.google.gwt.junit.server.JUnitHostImpl</servlet-class>
	</servlet>
	
	<servlet>
	    <servlet-name>repositoryProxy</servlet-name>
	    <servlet-class>kornell.server.RepositoryProxyServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>jUnitHostImpl</servlet-name>
		<url-pattern>/Kornell/junithost/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	    <servlet-name>repositoryProxy</servlet-name>
	    <url-pattern>/repository/*</url-pattern>	    
	</servlet-mapping>
</web-app>