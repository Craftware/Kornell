<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<g:FlowPanel>
		<g:FlowPanel ui:field="menuBar" addStyleNames="menuBar">
			<g:Image url="skins/first/icons/logo.png" addStyleNames="imgMenuBar"/>
		    <g:FlowPanel addStyleNames="buttons">
				<b:Button ui:field="btnFake" addStyleNames="shy menuBarButton first"></b:Button>	
				<b:Button ui:field="btnHome" addStyleNames="shy menuBarButton"></b:Button>		
				<b:Button ui:field="btnNotifications" addStyleNames="shy menuBarButton"></b:Button>		
				<b:Button ui:field="btnMessages" addStyleNames="shy menuBarButton"></b:Button>		
				<b:Button ui:field="btnHelp" addStyleNames="shy menuBarButton"></b:Button>		
				<b:Button ui:field="btnMenu" addStyleNames="shy menuBarButton"></b:Button>		
				<b:Button ui:field="btnExit" addStyleNames="shy menuBarButton last"></b:Button>		
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>