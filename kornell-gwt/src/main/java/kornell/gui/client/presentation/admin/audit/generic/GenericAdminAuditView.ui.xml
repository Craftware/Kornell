<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">

	<g:FlowPanel ui:field="adminHomePanel" addStyleNames="deanHome">
	    <g:FlowPanel ui:field="entitiesChangedPanel" addStyleNames="courseClassesHeaderWrapper">
		    <g:FlowPanel addStyleNames="left">
		      <g:HTMLPanel addStyleNames="title">Auditoria</g:HTMLPanel>
		      <g:HTMLPanel addStyleNames="subTitle">Escolha abaixo a entidade que deseja gerenciar auditar</g:HTMLPanel>
		    </g:FlowPanel>
		</g:FlowPanel>
	    <g:FlowPanel ui:field="entitiesChangedWrapper" addStyleNames="courseClassesHeaderWrapper">
		</g:FlowPanel>
	    <g:FlowPanel ui:field="diffWrapper" addStyleNames="diffWrapper">
		    <g:HTMLPanel>
			  <div class="json-input">
			    <div>
			      <span class="title">Antes</span>
			      <textarea class="json-input-left"></textarea>
			    </div>
			    <div>
			      <span class="title">Depois</span>
			      <textarea class="json-input-right"></textarea>
			    </div>
			  </div>
			  <div class="visualdiffWrapper">
			    <span class="title">Comparação</span>
			    <p class="visualdiff"></p>
			  </div>
			</g:HTMLPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>