<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<b:Modal ui:field="passwordChangeModal" backdrop="STATIC" addStyleNames="forgotPasswordModal">
		<g:FlowPanel>
			<g:FlowPanel ui:field="passwordChangeFields" addStyleNames="fieldsWrapper marginTop25"></g:FlowPanel>		
			<g:FlowPanel addStyleNames="standardButtonBar">
				<g:Button ui:field="btnOK" addStyleNames="btnAction btnStandard"/>
				<g:Button ui:field="btnCancel" addStyleNames="btnNotSelected btnStandard"/>
			</g:FlowPanel>
		</g:FlowPanel>
	</b:Modal>
</ui:UiBinder>