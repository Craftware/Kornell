<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:ScrollPanel>
		<g:FlowPanel ui:field="coursesPanel" addStyleNames="pnlRight">
			<g:FlowPanel addStyleNames="btnPanel">
				<g:Button ui:field="btnCoursesToAcquire" addStyleNames="btnCourses"/>
				<g:Button ui:field="btnCoursesFinished" addStyleNames="btnCourses"/>
				<g:Button ui:field="btnCoursesToStart" addStyleNames="btnCourses"/>
				<g:Button ui:field="btnCoursesInProgress" addStyleNames="btnCourses"/>
				<g:Button ui:field="btnCoursesAll" addStyleNames="btnCourses borderNone"/>
				<g:Label ui:field="headerText" addStyleNames="headerText"></g:Label>
			</g:FlowPanel>
			<g:FlowPanel ui:field="pnlCourses" addStyleNames="pnlCourses"/>
		</g:FlowPanel>
  </g:ScrollPanel>
</ui:UiBinder> 