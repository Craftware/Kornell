<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">

  <g:FlowPanel addStyleNames="courseClassAdmins">
		<g:HTMLPanel addStyleNames="title">Editar Administradores da Turma</g:HTMLPanel>
		<g:Label addStyleNames="subTitle">Escolha na lista abaixo os administradores desta turma. Os administradores da turma podem matricular alunos e responder as mensagens da turma, mas não poderão alterar as configurações da turma ou adicionar novos administradores.</g:Label>
		<b:Form ui:field="form" addStyleNames="profileForm">
			<g:FlowPanel ui:field="adminsFields" addStyleNames="fieldsWrapper marginTop25">		
			</g:FlowPanel>		
			<g:FlowPanel addStyleNames="standardButtonBar">
				<g:Button ui:field="btnOK" addStyleNames="btnAction btnStandard"/>
				<g:Button ui:field="btnCancel" addStyleNames="btnNotSelected btnStandard"/>
			</g:FlowPanel>
		</b:Form>		
	</g:FlowPanel>

</ui:UiBinder>