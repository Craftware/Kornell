<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<g:HorizontalPanel>
		<g:VerticalPanel ui:field="pnlMenuLeft"/>
   		<g:ScrollPanel>
			<g:FlowPanel>
				<g:FlowPanel>
					<g:Button ui:field="btnCoursesToAcquire" addStyleNames="btnCourses"/>
					<g:Button ui:field="btnCoursesFinished" addStyleNames="btnCourses"/>
					<g:Button ui:field="btnCoursesToStart" addStyleNames="btnCourses"/>
					<g:Button ui:field="btnCoursesInProgress" addStyleNames="btnCourses"/>
					<g:Button ui:field="btnCoursesAll" addStyleNames="btnCourses borderNone"/>
				</g:FlowPanel>
				
				<g:VerticalPanel ui:field="pnlInProgress" addStyleNames="pnlCoursesWrapper">
					<g:HorizontalPanel>
						<g:Image url="skins/first/icons/courses.png" addStyleNames="imgPnlCourses"/>
						<g:Label ui:field="labelCoursesInProgress" addStyleNames="labelPnlCourses"/>
						<g:Label ui:field="labelCoursesInProgressCount" addStyleNames="labelPnlCourses labelPnlCoursesHighlight"/>
					</g:HorizontalPanel>
					<g:HorizontalPanel ui:field="pnlCoursesInProgress" addStyleNames="pnlCourses"/>
				</g:VerticalPanel>
				
				<g:VerticalPanel ui:field="pnlToStart" addStyleNames="pnlCoursesWrapper">
					<g:HorizontalPanel>
						<g:Image url="skins/first/icons/courses.png" addStyleNames="imgPnlCourses"/>
						<g:Label ui:field="labelCoursesToStart" addStyleNames="labelPnlCourses"/>
						<g:Label ui:field="labelCoursesToStartCount" addStyleNames="labelPnlCourses labelPnlCoursesHighlight"/>
					</g:HorizontalPanel>
				    <g:HorizontalPanel ui:field="pnlCoursesToStart" addStyleNames="pnlCourses"/>
				</g:VerticalPanel>
				    
				    
				<g:VerticalPanel ui:field="pnlToAcquire" addStyleNames="pnlCoursesWrapper">
					<g:HorizontalPanel>
						<g:Image url="skins/first/icons/courses.png" addStyleNames="imgPnlCourses"/>
				    	<g:Label ui:field="labelCoursesToAcquire" addStyleNames="labelPnlCourses"/>
						<g:Label ui:field="labelCoursesToAcquireCount" addStyleNames="labelPnlCourses labelPnlCoursesHighlight"/>
					</g:HorizontalPanel>
				    <g:HorizontalPanel ui:field="pnlCoursesToAcquire" addStyleNames="pnlCourses"/>
				</g:VerticalPanel>
				    
				    
				<g:VerticalPanel ui:field="pnlFinished" addStyleNames="pnlCoursesWrapper">
					<g:HorizontalPanel>
						<g:Image url="skins/first/icons/courses.png" addStyleNames="imgPnlCourses"/>
				    	<g:Label ui:field="labelCoursesFinished" addStyleNames="labelPnlCourses"/>
						<g:Label ui:field="labelCoursesFinishedCount" addStyleNames="labelPnlCourses labelPnlCoursesHighlight"/>
					</g:HorizontalPanel>
				    <g:HorizontalPanel ui:field="pnlCoursesFinished" addStyleNames="pnlCourses"/>
				</g:VerticalPanel>
			</g:FlowPanel>
   		</g:ScrollPanel>
	</g:HorizontalPanel>
</ui:UiBinder>










