<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<g:FlowPanel addStyleNames="reportWrapper">
		<g:FlowPanel addStyleNames="itemPanel">
			<g:Image ui:field="certificationIcon" addStyleNames="reportIcon"></g:Image>
			<g:Label ui:field="lblName" addStyleNames="lblName"></g:Label>
			<g:Label ui:field="lblDescription" addStyleNames="lblDescription"></g:Label>
		</g:FlowPanel>
		<g:Anchor ui:field="lblGenerate" addStyleNames="lblGenerate"></g:Anchor>
		<g:Anchor ui:field="lblDownload" addStyleNames="lblDownload"></g:Anchor>
	</g:FlowPanel>
</ui:UiBinder> 
