<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">

	<g:FlowPanel ui:field="adminHomePanel" addStyleNames="deanHome">
		<g:FlowPanel ui:field="coursesPanel" addStyleNames="courseClassesHeaderWrapper">
			<g:FlowPanel addStyleNames="left">
				<g:Label ui:field="title" addStyleNames="title"></g:Label>
				<g:HTMLPanel addStyleNames="subTitle">Escolha abaixo o curso que deseja gerenciar</g:HTMLPanel>
			</g:FlowPanel>
			<b:Button ui:field="btnAddCourse" addStyleNames="btnAction btnIcon right" size="MINI" icon="PLUS_SIGN" />
		</g:FlowPanel>
		<g:FlowPanel ui:field="coursesWrapper" addStyleNames="courseClassesHeaderWrapper">
		</g:FlowPanel>
		<g:FlowPanel ui:field="createCoursePanel">
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>