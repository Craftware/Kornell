<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<g:FlowPanel addStyleNames="certificationWrapper">
		<g:FlowPanel addStyleNames="itemPanel">
			<g:Image ui:field="certificationIcon" addStyleNames="certificationIcon"></g:Image>
			<g:Label ui:field="lblName" addStyleNames="lblName"></g:Label>
		</g:FlowPanel>
		<g:Label ui:field="lblStatus" addStyleNames="lblStatus"></g:Label>
		<g:Label ui:field="lblGrade" addStyleNames="lblGrade"></g:Label>
		<g:Anchor ui:field="lblActions" addStyleNames="lblActions"></g:Anchor>
	</g:FlowPanel>
</ui:UiBinder> 
