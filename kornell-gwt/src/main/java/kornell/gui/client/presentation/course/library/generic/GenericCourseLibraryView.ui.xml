<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<g:FlowPanel ui:field="libraryPanel" addStyleNames="libraryPanel">
		<g:FlowPanel ui:field="titlePanel" addStyleNames="titlePanel"></g:FlowPanel>
		<g:FlowPanel ui:field="buttonsPanel" addStyleNames="buttonsPanel">
		    <b:Button ui:field="btnAllFiles" addStyleNames="btnLibrary btnSelected"></b:Button>
		    <b:Button ui:field="btnMostViewed" addStyleNames="btnLibrary btnNotSelected"></b:Button>
		    <b:Button ui:field="btnTags" addStyleNames="btnLibrary btnNotSelected"></b:Button>
		    <b:Button ui:field="btnMyFiles" addStyleNames="btnLibrary btnNotSelected"></b:Button>
		    <b:Button ui:field="btnIncludeFile" addStyleNames="btnLibrary btnNotSelected"></b:Button>
		</g:FlowPanel>
		<g:FlowPanel ui:field="contentPanel" addStyleNames="contentPanel"></g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 